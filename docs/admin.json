          "200": {
            "description": "A successful response.",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Menu"
              }
            }
          }
        },
        "tags": [
          "sys/user"
        ],
        "security": [
          {
            "apiKey": []
