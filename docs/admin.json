        "tags": [
          "sys/base"
        ]
      }
    },
    "/api/sys/user/codes": {
      "get": {
        "summary": "获取用户权限码",
        "operationId": "UserCodes",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/string"
              }
            }
          }
        },
        "tags": [
          "sys/user"
        ],
        "security": [
          {
            "apiKey": []
          }
        ]
      }
    },
    "/api/sys/user/info": {
      "get": {
